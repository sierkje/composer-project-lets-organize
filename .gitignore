# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# Ignore files and directories for log files
/log/*
!/log/.gitkeep

# Ignore Drupal's site specific files and directories
/files/*
!/files/private/
/files/private/*
!/files/private/.gitkeep
!/files/config/
/files/config/*
!/files/config/sync
/files/config/sync/*
/files/config/sync/.gitkeep
/web/files/
!/web/files/.gitkeep
/web/sites/sites.php
/web/sites/*/*
!/web/sites/*/default.settings.php
!/web/sites/*/default.services.yml
!/web/sites/*/example.lets_organize.extend.yml


# Ignore SimpleTest multi-site environment.
web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/
